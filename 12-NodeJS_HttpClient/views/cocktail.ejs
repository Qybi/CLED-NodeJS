<div class="card">
  <img class="card-img-top" style="max-width: 200px;" src="<%= cocktail.strDrinkThumb %>" alt="Card image cap" />
  <div class="card-body">
    <div class="card-title"><%= cocktail.strDrink %></div>
    <div class="card-text"><%= cocktail.strInstructionsIT %></div>
    <div class="card-text" style="font-weight: 500;">Ingredients</div>
    <% ingredients.forEach(i => { %>
        <div class="card-text"><%= i.measure %> of <%= i.ingredient %> </div>
    <% }) %>
  </div>
</div>
